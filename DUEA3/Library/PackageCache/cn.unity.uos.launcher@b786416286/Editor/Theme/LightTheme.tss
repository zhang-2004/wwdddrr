:root {
    --launcher-background-main: #C8C8C8;
    --launcher-background-secondary: #BCBCBC;
    --launcher-background-input: #2A2A2A;
    --launcher-background-operate: #BDBDBD;
    --launcher-background-button: #515151;
    --launcher-text-main: #000000;
    --launcher-text-hint: rgba(0, 0, 0, 0.6);
    --launcher-text-button: #E4E4E4;
    --launcher-text-link: #0967CB;
    --launcher-border: #A9A9A9;
    --launcher-background-transparent: transparent;
    --launcher-background-dropdown: #585858;
    --launcher-background-tint: #B2B2B2;
    --launcher-upgrade-button: url('../Sprites/UpgradeLight.png');
    --launcher-button-hover: rgba(0, 0, 0, 0.03);
    --launcher-button-outline: rgba(0, 0, 0, 0.18);
    --launcher-icon-background-tint: #565656;
    --launcher-uos-logo: url('../Sprites/UOSLogoLight.png');
    --launcher-highlight: #000000;
}