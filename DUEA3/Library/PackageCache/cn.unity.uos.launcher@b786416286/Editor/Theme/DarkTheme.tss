:root {
    --launcher-background-main: #3C3C3C;
    --launcher-background-secondary: #313131;
    --launcher-background-input: #2A2A2A;
    --launcher-background-operate: #343434;
    --launcher-background-button: #515151;
    --launcher-text-main: #D2D2D2;
    --launcher-text-hint: rgba(255,255,255, 0.5);
    --launcher-text-button: #E4E4E4;
    --launcher-text-link: #81B4FF;
    --launcher-border: #191919;
    --launcher-background-transparent: transparent;
    --launcher-background-dropdown: #585858;
    --launcher-background-tint: #B2B2B2;
    --launcher-upgrade-button: url('../Sprites/UpgradeDark.png');
    --launcher-button-hover: rgba(255, 255, 255, 0.03);
    --launcher-button-outline: rgba(0, 0, 0, 0.18);
    --launcher-icon-background-tint: #CDCDCD;
    --launcher-uos-logo: url('../Sprites/UOSLogoDark.png');
    --launcher-highlight: #D9D9D9;
}