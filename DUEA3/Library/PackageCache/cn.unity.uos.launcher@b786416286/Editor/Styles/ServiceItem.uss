.ItemRoot {
    flex-direction: row;
    align-items: center;
    height: 91px;
    width: 100%;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 1px;
    background-color: var(--launcher-background-main);
}

.ItemRoot > .ServiceIcon {
    width: 32px;
    height: 32px;
    align-items: auto;
    top: auto;
    background-image: url('../Sprites/Services/cn.unity.uos.passport.png');
    -unity-background-image-tint-color: var(--launcher-icon-background-tint);
    justify-content: flex-start;
    margin-left: 19px;
    margin-right: 14px;
    flex-shrink: 0;
}

.ItemRoot > .Info {
    flex-grow: 1;
    height: 100%;
    justify-content: flex-start;
}

.ItemRoot > .Info > .Header {
    flex-direction: row;
    align-items: center;
    margin-top: 16px;
}

.ItemRoot > .Info > .Description {
    color: var(--launcher-text-hint);
}

.ItemRoot > .Info > .Header > .Title {
    font-size: 21px;
    -unity-font-style: normal;
}

.ItemRoot > .Info > .Header > .Link {
    background-image: url('../Sprites/Link.png');
    margin-left: 12px;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    width: 12px;
    height: 12px;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    background-color: var(--launcher-background-transparent);
}

.ItemRoot > .Buttons {
    flex-grow: 0;
    flex-shrink: 0;
    background-color: var(--launcher-background-main);
    flex-direction: row;
    justify-content: flex-end;
    align-items: center;
    width: auto;
    height: 100%;
    display: flex;
    padding-right: 28px;
}

.ItemRoot > .Buttons Button {
    border-width: 0;
}

.ItemRoot > .Buttons > .Dropdown {
    margin: 0;
    padding: 0;
}

.ItemRoot > .Buttons > .ManageButtons {
    flex-grow: 0;
    background-color: var(--launcher-background-operate);
    height: 24px;
    width: auto;
    flex-direction: row;
    align-items: center;
    justify-content: space-around;
    align-self: auto;
    flex-shrink: 0;
    border-radius: 2px;
    margin-right: 10px;
    padding: 0;
    border-width: 1px;
    border-color: var(--launcher-button-outline);
}

.ItemRoot > .Buttons .LButton {
    width: 28px;
    height: 24px;
    padding: 0;
    flex-shrink: 0;
    flex-grow: 0;
    background-color: var(--launcher-background-transparent);
    display: flex;
    justify-content: center;
    align-items: center;
}

.ItemRoot > .Buttons .LButton:hover {
    background-color: var(--launcher-button-hover);
}

.ItemRoot > .Buttons #LButtonIcon {
    width: 16px;
    height: 16px;
    margin: 0;
    padding: 0;
    flex-shrink: 0;
    flex-grow: 0;
    -unity-background-image-tint-color: var(--launcher-icon-background-tint);
}

.ItemRoot > .Buttons > .ManageButtons > #Install > #LButtonIcon {
    background-image: url('../Sprites/Install.png');
}

.ItemRoot > .Buttons > .ManageButtons > #Installed > #LButtonIcon {
    background-image: url('../Sprites/Installed.png');
}

.ItemRoot > .Buttons > .ManageButtons > #ConfigWeb > #LButtonIcon {
    background-image: url('../Sprites/ConfigWeb.png');
}

.ItemRoot > .Buttons > .ManageButtons > #Upload > #LButtonIcon {
    background-image: url('../Sprites/Upload.png');
}

.ItemRoot > .Buttons > .ManageButtons > #ConfigWindow > #LButtonIcon {
    background-image: url('../Sprites/ConfigWindow.png');
}
.ItemRoot > .Buttons > #HomePage > #LButtonIcon {
    background-image: url('../Sprites/HomePage.png');
}

.ItemRoot > .Buttons > #Docs > #LButtonIcon {
    background-image: url('../Sprites/Docs.png');
}

.ItemRoot > .Buttons > .ManageButtons > #ServiceSample > #LButtonIcon {
    background-image: url('../Sprites/Sample.png');
}

.ItemRoot > .Buttons > .ManageButtons > #ServiceSampleImported > #LButtonIcon {
    background-image: url('../Sprites/SampleImported.png');
}

.ItemRoot > .Buttons > .ManageButtons > #ServiceSampleUrl > #LButtonIcon {
    background-image: url('../Sprites/Sample.png');
}

#LoadingIcon {
    width: 12px;
    height: 12px;
    margin: 0;
    padding: 0;
    flex-shrink: 0;
    flex-grow: 0;
    -unity-background-image-tint-color: var(--launcher-icon-background-tint);
    background-image: url('../Sprites/Loading.png');
}

.ItemRoot > .Buttons > .EnableButtons {
    width: 91px;
    height: 24px;
    flex-shrink: 0;
    flex-grow: 0;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    margin-right: 10px;
    background-color: var(--launcher-background-operate);
    border-width: 1px;
    border-color: var(--launcher-button-outline);
    border-radius: 2px;
}

.ItemRoot > .Buttons > .EnableButtons > .Enable {
    width: 62px;
    flex-shrink: 0;
    flex-grow: 0;
    margin: 0;
    background-color: var(--launcher-background-operate);
}

.ItemRoot > .Buttons > .EnableButtons > .Enable:hover {
    background-color: var(--launcher-button-hover);
}

.ItemRoot > .Buttons > .EnableButtons > .Dropdown {
    width: 27px;
    flex-shrink: 0;
    flex-grow: 0;
    display: flex;
    flex-direction: row;
    justify-content: center;
    margin: 0;
    padding: 0;
    background-color: var(--launcher-background-operate);
}

.ItemRoot > .Buttons > .EnableButtons > .Dropdown:hover {
    background-color: var(--launcher-button-hover);
}

.ItemRoot > .Buttons > .EnableButtons > .Dropdown > .Icon {
    background-image: url('../Sprites/Arrow_drop_down.png');
    width: 16px;
    height: 20px;
    -unity-background-image-tint-color: var(--launcher-background-tint);
    background-color: var(--launcher-background-transparent);
}

.ItemRoot > .Info > .InstallInfo {
    color: var(--launcher-text-hint);
    flex-direction: row;
    align-items: center;
    justify-content: flex-start;
    margin-top: 2px;
}

.ItemRoot > .Info > .InstallInfo > .VersionAndDetail {
    background-color: var(--launcher-background-transparent);
    color: var(--launcher-text-link);
    border-width: 0;
    margin: 0;
    cursor: link;
}

#UpgradeButton {
    width: 24px;
    height: 24px;
    border-radius: 12px;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: var(----launcher-background-transparent);
}

#UpgradeButton:hover {
    background-color: var(--launcher-button-hover);
}

#UpgradeButton > #LButtonIcon {
    background-image: var(--launcher-upgrade-button);
    width: 16px;
    height: 16px;
}

.ItemRoot > .Buttons > .ManageButtons Button:hover {
    background-color: rgba(255, 255, 255, 0.03);
}

.ItemRoot > .Buttons Button:hover {
    background-color: rgba(255, 255, 255, 0.03);
}

#EnableLinkButton {
    background-image: url(../Sprites/Link.png);
}