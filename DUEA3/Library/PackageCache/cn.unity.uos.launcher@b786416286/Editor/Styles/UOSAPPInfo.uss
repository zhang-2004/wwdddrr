.UOSAPPInfo {
    flex-grow: 1;
    height: auto;
    margin: 0;
    padding: 20px 26px 20px 26px;
    background-color: var(--launcher-background-secondary);
    height: 200px;
}

.UOSAPPInfo > .Fields {
    flex-grow: 1;
}
.UOSAPPInfo > .Fields > .AppID {
    height: 20px;
    margin: 0 0 4px;
}

.UOSAPPInfo > .Fields > .AppSecret {
    height: 20px;
    margin: 0 0 4px;
}

.UOSAPPInfo > .Fields > .AppServiceSecret {
    height: 20px;
    margin: 0 0 4px;
}

.FixSettingsButton {
    background-color: var(--launcher-background-transparent);
    color: var(--launcher-text-link);
    border-width: 0;
    margin: 0;
    display: none;
    padding-left: 0;
    padding-right: 0;
    cursor: link;
}
